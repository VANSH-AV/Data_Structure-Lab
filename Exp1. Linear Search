#include<stdio.h>
void main() {
	int n,key , pos=0 ,flag=0;
	scanf("%d",&n);

	int arr[n];
	for (int i = 0; i<n;i++){
	scanf("%d",&arr[i]);
		}
	scanf("%d", &key);
	for(int j = 0; j<n ; j++){
		if (arr[j] == key){
			pos = j;
			flag = 1;
		}
	}
	if (flag == 1) {
		printf("found at position %d\n", pos);
	} else {
		printf("%d not found\n", key);
	}
}
